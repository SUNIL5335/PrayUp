{
  "expo": {
    "name": "PrayUp",
    "slug": "prayup",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/img/lgo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.pray.prayup"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": ""
  }
}